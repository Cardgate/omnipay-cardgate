{
    "name": "cardgate/omnipay-cardgate",
    "description": "Cardgate gateway for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "cardgate"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Cardgate",
            "email": "tech@cardgate.com"
        },
        {
            "name": "Cardgate",
            "homepage": "https://cardgate.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\Cardgate\\" : "src/"
        }
    },
    "require": {
        "php": "^5.6|^7",
        "omnipay/common": "^3",
        "php-http/guzzle6-adapter": "^1.1|^2"
    },
    "require-dev": {
        "omnipay/tests": "^3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "prefer-stable": true
}
